page{
        width: 100%;
        height: 100%;
        background: linear-gradient(to bottom, #1f1818 , rgb(100, 93, 93))
}
.map-box{
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    position: relative;
}
.map{
    width: 100%;
    height: 957rpx;
    position: absolute;
    border-bottom:solid 1rpx #000;
    z-index: 200;
    top:0;
    left:0;
}
.text{
    border-top:solid 1rpx #666;
    position: absolute;
    bottom: 100rpx;
    z-index: 100;
    width: 100%;
}
.provinces,.citys{
    margin-right:10rpx;
    font-size: 30rpx;
}
.cin,.prm{
    color: red;
}
.back{
    width: 100%;
    height: 100%;
}
ec-canvas{
    width: 100%;
    height: 100%;
}